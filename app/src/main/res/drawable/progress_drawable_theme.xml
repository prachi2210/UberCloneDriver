<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip"/>
            <solid android:color="#FFFFFF"/>

            <gradient
                android:startColor="#d8d9dd"
                android:centerColor="#d8d9dd"
                android:centerY="1.0"
                android:endColor="#d8d9dd"
                android:angle="270"
                />

        </shape>
    </item>


    <item android:id="@android:id/progress">
        <clip
            android:clipOrientation="vertical"
            android:gravity="bottom">
            <shape>
                <corners android:radius="5dip"/>


                <gradient

                    android:startColor="#581684"
                    android:centerColor="#922e77"
                    android:centerY="1.0"
                    android:endColor="#ac3c72"
                    android:angle="270"
                    />
            </shape>
        </clip>
    </item>
</layer-list>